{
  "name": "kysely-singlestore",
  "version": "0.2.2",
  "description": "Kysely dialect for Singlestore (formerly MemSQL)",
  "repository": "https://github.com/igalklebanov/kysely-singlestore.git",
  "homepage": "https://github.com/igalklebanov/kysely-singlestore",
  "author": "Igal Klebanov <igalklebanov@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "kysely",
    "singlestore",
    "memsql",
    "mysql",
    "driver",
    "dialect"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint": "eslint src --ext .ts",
    "prepack": "pnpm run lint && pnpm run build",
    "test": "jest --passWithNoTests",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "kysely": ">= 0.7.8 < 1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.3",
    "@swc/jest": "^0.2.22",
    "@tsconfig/node14": "^1.0.3",
    "@types/jest": "^29.0.3",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.3",
    "kysely": "^0.21.6",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prettier-plugin-pkg": "^0.17.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3",
    "undici": "^5.10.0"
  },
  "sideEffects": false
}
